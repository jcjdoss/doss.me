<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Clawd RPG Dashboard</title>
    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }
        
        body {
            font-family: 'Courier New', monospace;
            background: linear-gradient(135deg, #1e3c72 0%, #2a5298 100%);
            color: #fff;
            padding: 20px;
            min-height: 100vh;
        }
        
        .container {
            max-width: 1200px;
            margin: 0 auto;
        }
        
        h1 {
            text-align: center;
            font-size: 3em;
            margin-bottom: 10px;
            text-shadow: 2px 2px 4px rgba(0,0,0,0.5);
        }
        
        .subtitle {
            text-align: center;
            font-size: 1.2em;
            opacity: 0.8;
            margin-bottom: 30px;
        }
        
        .stats-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            gap: 20px;
            margin-bottom: 30px;
        }
        
        .stat-card {
            background: rgba(255,255,255,0.1);
            border-radius: 10px;
            padding: 20px;
            backdrop-filter: blur(10px);
            border: 1px solid rgba(255,255,255,0.2);
        }
        
        .stat-label {
            font-size: 0.9em;
            opacity: 0.7;
            margin-bottom: 5px;
        }
        
        .stat-value {
            font-size: 2.5em;
            font-weight: bold;
            margin-bottom: 10px;
        }
        
        .stat-bar {
            height: 20px;
            background: rgba(255,255,255,0.2);
            border-radius: 10px;
            overflow: hidden;
        }
        
        .stat-bar-fill {
            height: 100%;
            transition: width 1s ease-out;
        }
        
        .chart-container {
            background: rgba(255,255,255,0.1);
            border-radius: 10px;
            padding: 20px;
            backdrop-filter: blur(10px);
            border: 1px solid rgba(255,255,255,0.2);
            margin-bottom: 20px;
        }
        
        canvas {
            max-width: 100%;
        }
        
        .color-vrl { background: linear-gradient(90deg, #ff006e, #ff5757); }
        .color-spd { background: linear-gradient(90deg, #ffbe0b, #ffd60a); }
        .color-rch { background: linear-gradient(90deg, #00b4d8, #90e0ef); }
        .color-tru { background: linear-gradient(90deg, #06ffa5, #00cc88); }
        .color-wis { background: linear-gradient(90deg, #4361ee, #7209b7); }
        .color-cre { background: linear-gradient(90deg, #ff006e, #8338ec); }
    </style>
</head>
<body>
    <div class="container">
        <h1>ðŸŽ® CLAWD RPG DASHBOARD</h1>
        <p class="subtitle">Oracle â€¢ Level <span id="level">?</span></p>
        
        <div class="stats-grid">
            <div class="stat-card">
                <div class="stat-label">VRL - Viral</div>
                <div class="stat-value" id="vrl-value">0</div>
                <div class="stat-bar">
                    <div class="stat-bar-fill color-vrl" id="vrl-bar" style="width: 0%"></div>
                </div>
            </div>
            
            <div class="stat-card">
                <div class="stat-label">SPD - Speed</div>
                <div class="stat-value" id="spd-value">0</div>
                <div class="stat-bar">
                    <div class="stat-bar-fill color-spd" id="spd-bar" style="width: 0%"></div>
                </div>
            </div>
            
            <div class="stat-card">
                <div class="stat-label">RCH - Reach</div>
                <div class="stat-value" id="rch-value">0</div>
                <div class="stat-bar">
                    <div class="stat-bar-fill color-rch" id="rch-bar" style="width: 0%"></div>
                </div>
            </div>
            
            <div class="stat-card">
                <div class="stat-label">TRU - Trust</div>
                <div class="stat-value" id="tru-value">0</div>
                <div class="stat-bar">
                    <div class="stat-bar-fill color-tru" id="tru-bar" style="width: 0%"></div>
                </div>
            </div>
            
            <div class="stat-card">
                <div class="stat-label">WIS - Wisdom</div>
                <div class="stat-value" id="wis-value">0</div>
                <div class="stat-bar">
                    <div class="stat-bar-fill color-wis" id="wis-bar" style="width: 0%"></div>
                </div>
            </div>
            
            <div class="stat-card">
                <div class="stat-label">CRE - Creative</div>
                <div class="stat-value" id="cre-value">0</div>
                <div class="stat-bar">
                    <div class="stat-bar-fill color-cre" id="cre-bar" style="width: 0%"></div>
                </div>
            </div>
        </div>
        
        <div class="chart-container">
            <h2>Stats Over Time</h2>
            <canvas id="statsChart"></canvas>
        </div>
    </div>
    
    <script src="https://cdn.jsdelivr.net/npm/chart.js@4"></script>
    <script>
        // Load stats data
        const statsData = {"timestamp":1770998269,"level":14,"vrl":99,"spd":98,"rch":94,"tru":99,"wis":99,"cre":98,"xp":13800};
        const historyData = [{"timestamp":1770998269,"level":14,"vrl":99,"spd":98,"rch":94,"tru":99,"wis":99,"cre":98}];
        
        // Update current stats
        document.getElementById('level').textContent = statsData.level;
        
        const stats = ['vrl', 'spd', 'rch', 'tru', 'wis', 'cre'];
        stats.forEach(stat => {
            const value = statsData.stats[stat.toUpperCase()];
            document.getElementById(`${stat}-value`).textContent = value;
            setTimeout(() => {
                document.getElementById(`${stat}-bar`).style.width = value + '%';
            }, 100);
        });
        
        // Create chart
        const ctx = document.getElementById('statsChart').getContext('2d');
        
        // Process history data
        const labels = historyData.map(d => {
            const date = new Date(d.timestamp * 1000);
            return date.toLocaleDateString();
        }).reverse();
        
        const datasets = [
            {
                label: 'VRL',
                data: historyData.map(d => d.vrl).reverse(),
                borderColor: '#ff006e',
                backgroundColor: 'rgba(255, 0, 110, 0.1)',
            },
            {
                label: 'SPD',
                data: historyData.map(d => d.spd).reverse(),
                borderColor: '#ffbe0b',
                backgroundColor: 'rgba(255, 190, 11, 0.1)',
            },
            {
                label: 'RCH',
                data: historyData.map(d => d.rch).reverse(),
                borderColor: '#00b4d8',
                backgroundColor: 'rgba(0, 180, 216, 0.1)',
            },
            {
                label: 'TRU',
                data: historyData.map(d => d.tru).reverse(),
                borderColor: '#06ffa5',
                backgroundColor: 'rgba(6, 255, 165, 0.1)',
            },
            {
                label: 'WIS',
                data: historyData.map(d => d.wis).reverse(),
                borderColor: '#4361ee',
                backgroundColor: 'rgba(67, 97, 238, 0.1)',
            },
            {
                label: 'CRE',
                data: historyData.map(d => d.cre).reverse(),
                borderColor: '#8338ec',
                backgroundColor: 'rgba(131, 56, 236, 0.1)',
            },
        ];
        
        new Chart(ctx, {
            type: 'line',
            data: {
                labels: labels,
                datasets: datasets
            },
            options: {
                responsive: true,
                plugins: {
                    legend: {
                        labels: {
                            color: '#fff'
                        }
                    }
                },
                scales: {
                    y: {
                        beginAtZero: true,
                        max: 100,
                        ticks: {
                            color: '#fff'
                        },
                        grid: {
                            color: 'rgba(255,255,255,0.1)'
                        }
                    },
                    x: {
                        ticks: {
                            color: '#fff'
                        },
                        grid: {
                            color: 'rgba(255,255,255,0.1)'
                        }
                    }
                }
            }
        });
    </script>
</body>
</html>
